{
  "expo": {
    "name": "MortgageConnect",
    "slug": "MortgageConnect",
    "version": "1.0.0",
    "scheme": "mortgageconnect",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow MortgageConnect to access your photos to share in chat.",
          "cameraPermission": "Allow MortgageConnect to use your camera to take photos for chat."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ayxn07.MortgageConnect",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
"android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ayxn07.MortgageConnect",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "resize"
    }
  }
}
